                </svg>
              </a>
              <a href="/" class="nav_show_two">
                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor"
                  class="bi bi-headset" viewBox="0 0 16 16">
                  <path
                    d="M8 1a5 5 0 0 0-5 5v1h1a1 1 0 0 1 1 1v3a1 1 0 0 1-1 1H3a1 1 0 0 1-1-1V6a6 6 0 1 1 12 0v6a2.5 2.5 0 0 1-2.5 2.5H9.366a1 1 0 0 1-.866.5h-1a1 1 0 1 1 0-2h1a1 1 0 0 1 .866.5H11.5A1.5 1.5 0 0 0 13 12h-1a1 1 0 0 1-1-1V8a1 1 0 0 1 1-1h1V6a5 5 0 0 0-5-5" />
                </svg>
              </a>
            </div>
            <!-- Navbar -->
            <t t-call="website.navbar_nav">
              <t t-set="_nav_class" t-valuef="cider_navbar_nav" />
              <t t-foreach="website.menu_id.child_id" t-as="submenu">
                <t t-call="website.submenu">
                  <t t-set="item_class" t-valuef="nav-item" />
                  <t t-set="link_class" t-valuef="nav-link" />
                </t>
              </t>
            </t>

          </div>
        </div>
        <!-- Modal -->
        <div class="modal fade modal-fullscreen py-0" id="navSearch" data-bs-backdrop="static"
          data-bs-keyboard="false" tabindex="-1" aria-labelledby="navSearchLabel" aria-hidden="true">
          <div class="modal-dialog modal-fullscreen py-0">
            <div class="modal-content">
              <div class="modal-header p-0">
                <div class="container d-flex py-3">
                  <div class=" w-100 row justify-content-center">
                    <div class="col-sm-9 d-flex hr_line pt-5 px-0">
                      <div class="for_search_nav">
                        <t t-call="website.placeholder_header_search_box">
                            <t t-set="_classes" t-valuef="mb-3"/>
                            <t t-set="_input_classes" t-valuef="rounded-start-pill text-bg-light ps-3"/>
                            <t t-set="_submit_classes" t-valuef="rounded-end-pill bg-o-color-3 pe-3"/>
                            <t t-set="limit" t-valuef="0"/>
                        </t>
                       </div>
                      <!-- <button type="submit">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                          fill="currentColor"
